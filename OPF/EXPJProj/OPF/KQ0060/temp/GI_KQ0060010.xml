at></arg><arg><name>arg 12</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>DOWNLOAD_FILE</param><format></format></arg><arg><name>arg 13</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>UNCONFIRM_ODR_QTY</param><format></format></arg><arg><name>arg 14</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>ODR_REM_QTY</param><format></format></arg></cells><entities><comment>エンティティ情報</comment><entity><description></description></entity></entities></method><method><name>Close</name><jname>閉じる</jname><description></description><BaseMethod>onClick</BaseMethod><define>AUTO</define><event-type>none</event-type><event-options><comment></comment></event-options><args><comment>引数設定情報</comment><arg><name>arg 1</name><jname>ボタン名 (日本語、省略可)</jname><datatype>String</datatype><in>0</in><ismed>0</ismed><param>閉じる</param></arg><arg><name>arg 2</name><jname>ボタンＩＤ (必須)</jname><datatype>String</datatype><in>0</in><ismed>0</ismed><param>Close</param></arg><arg><name>arg 3</name><jname>移動先JSP (省略可)</jname><datatype>String</datatype><in>0</in><ismed>0</ismed><param></param></arg></args><cells><comment>mediator情報</comment><arg><name>arg 1</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>IN_ITEM_CD</param><format></format></arg><arg><name>arg 2</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>IN_DATE</param><format></format></arg><arg><name>arg 3</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>ITEM_CD</param><format></format></arg><arg><name>arg 4</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>ITEM_NAME</param><format></format></arg><arg><name>arg 5</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>SUM_ODR_QTY</param><format></format></arg><arg><name>arg 6</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>PLN_QTY_REMAIN</param><format></format></arg><arg><name>arg 7</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>SHIP_QTY</param><format></format></arg><arg><name>arg 8</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>SUM_OD</param><format></format></arg><arg><name>arg 9</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>SUM_STOCK_ON_HAND_QTY</param><format></format></arg><arg><name>arg 10</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>QTY_DIFF</param><format></format></arg><arg><name>arg 11</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>PKG_UNIT</param><format></format></arg><arg><name>arg 12</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>DOWNLOAD_FILE</param><format></format></arg><arg><name>arg 13</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>UNCONFIRM_ODR_QTY</param><format></format></arg><arg><name>arg 14</name><datatype>String</datatype><in>out</in><ismed>1</ismed><param>ODR_REM_QTY</param><format></format></arg></cells><entities><comment>エンティティ情報</comment><entity><description></description></entity></entities></method></mets></ctrl></ctrls><entities><comment></comment><entity><name>Select</name><BaseClass>dbms</BaseClass><define>AUTO</define><create><enable>false</enable><status>inactive</status></create><check><enable>false</enable><status>inactive</status></check><read><enable>true</enable><status>active</status><name>Select</name><BaseClass>dbms</BaseClass><WizType>2</WizType><define>AUTO</define><params><dsn>EXPJ</dsn><user>EXPJ</user><password>expj</password><JDBC>sun.jdbc.odbc.JdbcOdbcDriver</JDBC><jdbc_prefix>jdbc:odbc:</jdbc_prefix><useTransaction>false</useTransaction><sqlcmd>"SELECT "
+"  M_ITEM.ITEM_CD AS ITEM_CD, "
+"  M_ITEM.ITEM_NAME AS ITEM_NAME, "
+"  NVL(A.SUM_ODR_QTY, 0) AS SUM_ODR_QTY, "
+"  NVL(A.UNCONFIRM_ODR_QTY, 0) AS UNCONFIRM_ODR_QTY, "
+"  NVL(F.PLN_QTY_REMAIN, 0) AS PLN_QTY_REMAIN, "
+"  NVL(A.SHIP_QTY, 0) AS SHIP_QTY, "
+"  NVL(A.SUM_ODR_QTY, 0) - NVL(A.SHIP_QTY, 0) + NVL(A.UNCONFIRM_ODR_QTY, 0) - NVL(D.DEPO_SHIP_QTY, 0) AS ODR_REM_QTY, "
+"  NVL(E.SUM_OD, 0) AS SUM_OD, "
+"  NVL(B.SUM_STOCK_ON_HAND_QTY, 0) + NVL(C.SUM_STOCK_ON_HAND_QTY, 0) AS SUM_STOCK_ON_HAND_QTY, "
+"  (NVL(A.SUM_ODR_QTY, 0) - NVL(A.SHIP_QTY, 0) + NVL(A.UNCONFIRM_ODR_QTY, 0) - NVL(D.DEPO_SHIP_QTY, 0) - NVL(E.SUM_OD, 0) - NVL(B.SUM_STOCK_ON_HAND_QTY, 0) - NVL(C.SUM_STOCK_ON_HAND_QTY, 0))*(-1) AS QTY_DIFF, "
+"  M_ITEM.STOCK_UNIT AS PKG_UNIT "
+"FROM "
+"  M_ITEM, "
+"  M_ITEM_SPEC, "
+"  ( "
+"    SELECT "
+"      T_ODR_CTL.ITEM_CD AS ITEM_CD, "
+"      SUM(DECODE(T_ODR.ODR_TYP, 3, NVL(T_ODR.ODR_QTY, 0))) AS SUM_ODR_QTY, "
+"      SUM(DECODE(T_ODR.ODR_TYP, 2, NVL(T_ODR.REMAIN_UNCONFIRM_ODR_QTY, 0))) AS UNCONFIRM_ODR_QTY, "
+"      SUM(DECODE(T_ODR.ODR_TYP, 3, NVL(T_ODR.TOTAL_SHIP_QTY, 0))) AS SHIP_QTY, "
+"      SUM(DECODE(T_ODR.ODR_TYP, 3, NVL(T_ODR.ODR_QTY, 0) - NVL(T_ODR.TOTAL_SHIP_QTY, 0), 2, NVL(T_ODR.REMAIN_UNCONFIRM_ODR_QTY, 0))) AS ODR_REM_QTY_NODEPO "
+"    FROM "
+"      T_ODR_CTL, "
+"      T_ODR "
+"    WHERE "
+"      T_ODR_CTL.ODR_CTL_NO = T_ODR.ODR_CTL_NO "
+"      AND T_ODR_CTL.DEL_FLG = 0 "
+"      AND T_ODR.DEL_FLG = 0 "
+"      AND(T_ODR.ODR_TYP = 3 "
+"      OR  T_ODR.ODR_TYP = 2) "
+"      AND T_ODR.DESINATED_DLV_DATE BETWEEN TO_DATE(?) AND TO_DATE(?) "
+"    GROUP BY "
+"      T_ODR_CTL.ITEM_CD "
+"  ) A, "
+"  ( "
+"    SELECT "
+"      T_ITEM_STOCK.ITEM_CD AS ITEM_CD, "
+"      NVL(SUM(T_ITEM_STOCK.STOCK_ON_HAND_QTY), 0) AS SUM_STOCK_ON_HAND_QTY "
+"    FROM "
+"      T_ITEM_STOCK, "
+"      M_WH "
+"    WHERE "
+"      T_ITEM_STOCK.WH_CD = M_WH.WH_CD "
+"      AND M_WH.MRP_FLG = 1 "
+"    GROUP BY "
+"      T_ITEM_STOCK.ITEM_CD "
+"  ) B, "
+"  ( "
+"    SELECT "
+"      T_JOB_ODR_CD_STOCK.ITEM_CD AS ITEM_CD, "
+"      NVL(SUM(T_JOB_ODR_CD_STOCK.STOCK_ON_HAND_QTY), 0) AS SUM_STOCK_ON_HAND_QTY "
+"    FROM "
+"      T_JOB_ODR_CD_STOCK, "
+"      M_WH "
+"    WHERE "
+"      T_JOB_ODR_CD_STOCK.WH_CD = M_WH.WH_CD "
+"      AND M_WH.MRP_FLG = 1 "
+"    GROUP BY "
+"      T_JOB_ODR_CD_STOCK.ITEM_CD "
+"  ) C, "
+"  ( "
+"    SELECT "
+"      T_SHIP_ODR.ITEM_CD AS ITEM_CD, "
+"      NVL(SUM(T_SHIP_ODR.SHIP_QTY), 0) AS DEPO_SHIP_QTY "
+"    FROM "
+"      T_SHIP_ODR "
+"    WHERE "
+"      DEPO_TRANSFER_PROC_FLG = 1 "
+"    GROUP BY "
+"      T_SHIP_ODR.ITEM_CD "
+"  ) D, "
+"  ( "
+"    SELECT "
+"      T_OD.ITEM_CD AS ITEM_CD, "
+"      SUM(NVL(T_OD.ODR_QTY, 0) - NVL(T_OD.TOTAL_RCV_QTY, 0)) AS SUM_OD "
+"    FROM "
+"      T_OD "
+"    WHERE "
+"      T_OD.OD_TYP = 2 "
+"      AND(T_OD.ODR_STS_TYP = 1 "
+"      OR  T_OD.ODR_STS_TYP = 2) "
+"      AND T_OD.PRD_DUE_DATE BETWEEN TO_DATE(?) AND TO_DATE(?) "
+"    GROUP BY "
+"      T_OD.ITEM_CD "
+"  ) E, "
+"  ( "
+"    SELECT "
+"      M_ITEM_SPEC.ITEM_CD AS ITEM_CD, "
+"      NVL(SUM(T_SALES_PLAN.PLN_QTY_REMAIN), 0) AS PLN_QTY_REMAIN "
+"    FROM "
+"      T_SALES_PLAN, "
+"      M_ITEM_SPEC "
+"    WHERE "
+"      T_SALES_PLAN.COMPANY_CD = M_ITEM_SPEC.COMPANY_CD "
+"      AND T_SALES_PLAN.PLN_QTY_REMAIN &gt; 0 "
+"      AND T_SALES_PLAN.CUST_CD = M_ITEM_SPEC.CUST_CD "
+"      AND T_SALES_PLAN.CUST_ITEM_CD = M_ITEM_SPEC.CUST_ITEM_CD "
+"      AND T_SALES_PLAN.DLT_FLG = 0 "
+"      AND(T_SALES_PLAN.PLN_STR_DATE BETWEEN TO_DATE(?) AND TO_DATE(?) "
+"      OR  T_SALES_PLAN.PLN_END_DATE BETWEEN TO_DATE(?) AND TO_DATE(?)) "
+"    GROUP BY "
+"      M_ITEM_SPEC.ITEM_CD "
+"  ) F "
+"WHERE "
+"  M_ITEM.ITEM_CD LIKE ? "
+"  AND M_ITEM.ITEM_CD = M_ITEM_SPEC.ITEM_CD "
+"  AND M_ITEM.ITEM_CD = A.ITEM_CD(+) "
+"  AND M_ITEM.ITEM_CD = B.ITEM_CD(+) "
+"  AND M_ITEM.ITEM_CD = C.ITEM_CD(+) "
+"  AND M_ITEM.ITEM_CD = D.ITEM_CD(+) "
+"  AND M_ITEM.ITEM_CD = E.ITEM_CD(+) "
+"  AND M_ITEM.ITEM_CD = F.ITEM_CD(+) "
+"GROUP BY "
+"  M_ITEM.ITEM_CD, "
+"  M_ITEM.ITEM_NAME, "
+"  NVL(A.SUM_ODR_QTY, 0), "
+"  NVL(A.UNCONFIRM_ODR_QTY, 0), "
+"  NVL(F.PLN_QTY_REMAIN, 0), "
+"  NVL(A.SHIP_QTY, 0), "
+"  NVL(A.SUM_ODR_QTY, 0) - NVL(A.SHIP_QTY, 0) + NVL(A.UNCONFIRM_ODR_QTY, 0) - NVL(D.DEPO_SHIP_QTY, 0), "
+"  NVL(E.SUM_OD, 0), "
